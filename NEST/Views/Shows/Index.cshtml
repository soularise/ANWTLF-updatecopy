@model IEnumerable<NEST.Models.ContentBody>

@{
    ViewBag.Title = "Shows";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<style>
#main-content h1{}
#main-content h2,h3{padding-left:10px;padding-top:15px;}

</style>

<h1>@ViewBag.Title</h1>

<div class="main-inner">
    <div id="main-content" class="radius" >
     <div style="display:inline-block; float:left;width:50%;">
@foreach (var item in Model)
{
           
   
    <h2>@Html.DisplayFor(modelItem => item.Title)</h2>
    <p>@Html.Raw(item.BodyText)<br />

    </p>

}
    </div>
    <div align="right">
    @Html.Partial("~/Views/Shared/_TwitterFeed.cshtml")
    </div>
</div></div>