@model IEnumerable<NEST.Models.ContentBody>

@{
    ViewBag.Title = "Video + Photos";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<div id="TitleDiv">
<h1>@ViewBag.Title</h1>
</div>
<div class="main-inner">
    <div id="main-content" class="radius">

    <div style="display:inline-block; float:left;width:50%;">

    @foreach (var item in Model) {
           
   
        <h2>@Html.DisplayFor(modelItem => item.Title)</h2>
        <h3>@Html.DisplayFor(modelItem => item.SubHeader)</h3>
        <p>@Html.Raw(item.BodyText)</p>



    }
    </div>
    @Html.Partial("~/Views/Shared/_TwitterFeed.cshtml")
    </div>
</div>
